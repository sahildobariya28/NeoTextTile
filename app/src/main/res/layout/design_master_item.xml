<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="android.view.View" />

        <variable
            name="design_model"
            type="com.example.texttile.data.model.DesignMasterModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="15dp">


        <com.example.texttile.presentation.ui.lib.new_swipe_feature.SwipeRevealLayout
            android:id="@+id/swipeRevealLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:dragEdge="right"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:mode="same_level">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingHorizontal="10dp">

                <LinearLayout
                    android:id="@+id/btn_edit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingHorizontal="10dp">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/edit" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/medium"
                        android:includeFontPadding="false"
                        android:text="Edit"
                        android:textSize="12sp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/btn_delete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingHorizontal="10dp">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/delete" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/medium"
                        android:includeFontPadding="false"
                        android:text="Delete"
                        android:textSize="12sp" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/view_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/border_bg_unselected"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="15dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:padding="8dp"
                        android:src="@drawable/vector" />

                    <TextView
                        android:id="@+id/txt_design_no"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:fontFamily="@font/regular"
                        android:gravity="center_vertical"
                        android:includeFontPadding="false"
                        android:paddingHorizontal="15dp"
                        android:text="@{design_model.design_no}"
                        android:textColor="@color/text_title_color"
                        android:textSize="15sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/regular"
                            android:includeFontPadding="false"
                            android:text="Card Length:"
                            android:textColor="@color/white_text_light"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/text_sample_card_length"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/regular"
                            android:includeFontPadding="false"
                            android:text="@{design_model.sample_card_len}"
                            android:textColor="@color/text"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/regular"
                            android:includeFontPadding="false"
                            android:text="Total Card:"
                            android:textColor="@color/white_text_light"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/text_total_card"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/regular"
                            android:includeFontPadding="false"
                            android:text="@{Integer.toString(design_model.total_card)}"
                            android:textColor="@color/text"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="end|center_vertical"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/btn_down"
                            android:layout_width="40dp"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:padding="15dp"
                            android:src="@drawable/down"
                            app:tint="@color/black" />

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/detail_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginVertical="15dp"
                        android:background="@color/white_text_light"
                        android:orientation="vertical" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="Reed:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_reed"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="@{Integer.toString(design_model.reed)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="Base Pick:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_base_pick"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="@{Integer.toString(design_model.base_pick)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="Base Card:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_base_card"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="@{Integer.toString(design_model.base_card)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/view_f1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() >= 1 ? View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F1:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(0)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />

                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/view_f3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() >= 3 ?  View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F3:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f3"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(2)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/view_f5"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() >= 5 ?  View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F5:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(4)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/view_f7"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() >= 7 ? View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F7:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f7"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(6)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="Avg Pick:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_avg_pick"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="@{Integer.toString(design_model.avg_pick)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="Total Length:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_total_length"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="@{Float.toString(design_model.total_len)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="Date"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_date"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="@{design_model.date}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/view_f2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() >= 2 ? View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F2:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f2"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(1)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/view_f4"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() >= 4 ? View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F4:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f4"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(3)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/view_f6"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() >= 6 ? View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F6:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f6"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(5)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/view_f8"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="@{design_model.f_list.size() == 8 ? View.VISIBLE : View.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:text="F8:"
                                    android:textColor="@color/text_light1"
                                    android:textSize="12sp"
                                    app:drawableLeftCompat="@drawable/bullets" />

                                <TextView
                                    android:id="@+id/text_f8"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:drawablePadding="10dp"
                                    android:fontFamily="@font/regular"
                                    android:includeFontPadding="false"
                                    android:paddingVertical="5dp"
                                    android:singleLine="true"
                                    android:text="@{design_model.getFlistText(7)}"
                                    android:textColor="@color/text_title_color"
                                    android:textSize="12sp" />
                            </LinearLayout>


                        </LinearLayout>

                    </LinearLayout>

                    <TextView
                        android:id="@+id/btn_show_jala_list"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_gravity="end"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/btn_bg"
                        android:fontFamily="@font/regular"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:paddingHorizontal="10dp"
                        android:text="Show Jala"
                        android:textColor="@color/white"
                        android:textSize="12sp" />


                </LinearLayout>

            </LinearLayout>

        </com.example.texttile.presentation.ui.lib.new_swipe_feature.SwipeRevealLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>
